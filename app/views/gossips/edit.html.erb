<h1>Edit Gossip</h1>

<%= form_for @gossip do |f| %>
  <div class="form-group">
    <label>Titre</label>
    <%= f.text_field :title, class: 'form-control' %>
  </div>
  <div class="form-group">
    <label>Content</label>
    <%= f.text_area :content, class: 'form-control' %>
  </div>
  <div class="form-group">
    <button class="btn btn-dark" type="submit">Modifier</button>
  </div>

<% end %>
